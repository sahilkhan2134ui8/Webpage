{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python",
      "config": {
        "pythonVersion": "3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "main.py"
    },
    {
      "src": "/about",
      "dest": "main.py"
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/"
    },
    {
      "source": "/about",
      "destination": "/about"
    }
  ]
}
